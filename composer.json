{
    "name": "jascha030/wp-hooks",
    "description": "wp-hooks",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jascha030",
            "email": "contact@jaschavanaalst.nl",
            "homepage": "https://github.com/jascha030"
        }
    ],
    "keywords": [
        "WordPress",
        "Hooks",
        "Actions",
        "Filters",
        "WP Plugin API",
        "Attributes"
    ],
    "homepage": "https://github.com/jascha030",
    "config": {},
    "bin": [],
    "autoload": {
        "psr-4": {
            "Jascha030\\Hooks\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jascha030\\Hooks\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0|^8.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "friendsofphp/php-cs-fixer": "^v3.8.0",
        "phpunit/phpunit": "^9.5"
    },
    "extra": {},
    "scripts": {
        "phpunit": "phpunit -c .phpunit.xml",
        "php-cs-fixer": "php-cs-fixer fix --config .php-cs-fixer.php",
        "test": "@phpunit",
        "format": "@php-cs-fixer"
    },
    "scripts-descriptions": {
        "test": "Runs phpunit with .phpunit.xml",
        "format": "Runs php-cs-fixer with .php-cs-fixer.php"
    }
}
